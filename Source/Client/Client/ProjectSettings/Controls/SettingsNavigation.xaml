<UserControl x:Class="YardLight.Client.ProjectSettings.Controls.SettingsNavigation"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:YardLight.Client.ProjectSettings.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Margin="5,0,0,0">
    <Border BorderBrush="DarkGray" BorderThickness="2" Padding="2,0">
        <StackPanel Orientation="Vertical">
            <Label>
                <Hyperlink Click="Hyperlink_Click" CommandParameter="ProjectSettings/ProjectSettings.xaml">Project Title</Hyperlink>
            </Label>
            <Label>
                <Hyperlink Click="Hyperlink_Click" CommandParameter="ProjectSettings/Itterations.xaml">Itterations</Hyperlink>
            </Label>
            <Label>
                <Hyperlink Click="Hyperlink_Click" CommandParameter="ProjectSettings/WorkItemTypes.xaml">Work Item Type</Hyperlink>
            </Label>
            <Label>
                <Hyperlink Click="Hyperlink_Click" CommandParameter="ProjectSettings/Members.xaml">Members</Hyperlink>
            </Label>
        </StackPanel>
    </Border>
</UserControl>
